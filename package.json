{
  "name": "testchimp-runner-action",
  "version": "1.0.4",
  "description": "GitHub Action for TestChimp AI test repair",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run package",
    "package": "ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.4.0",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^20.0.0",
    "playwright": "^1.40.0",
    "@playwright/test": "^1.40.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "esbuild": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "github-action",
    "testchimp",
    "playwright",
    "ai",
    "test-automation"
  ],
  "author": "TestChimp",
  "license": "MIT"
}
